@article{RozenasStukal2019,
  title = {How {{Autocrats Manipulate Economic News}}: {{Evidence}} from {{Russia}}'s {{State}}-{{Controlled Television}}},
  shorttitle = {How {{Autocrats Manipulate Economic News}}},
  author = {Rozenas, Arturas and Stukal, Denis},
  year = {2019},
  month = jul,
  volume = {81},
  pages = {982--996},
  publisher = {{The University of Chicago Press}},
  issn = {0022-3816},
  doi = {10.1086/703208},
  abstract = {Conventional wisdom says that autocrats manipulate news through censorship. But when it comes to economic affairs\textemdash a highly sensitive topic for modern autocrats\textemdash the government's ability to censor information effectively is limited, because citizens can benchmark the official news against their incomes, market prices, and other observables. We propose that instead of censoring economic facts, the media tactically frames those facts to make the government appear as a competent manager. Using a corpus of daily news reports from Russia's largest state-owned television network, we document extensive evidence supporting this prediction. Bad news is not censored, but it is systematically blamed on external factors, whereas good news is systematically attributed to domestic politicians. Such selective attribution is used more intensely in politically sensitive times (elections and protests) and when the leadership is already enjoying high popular support\textemdash consistent with the existing theories of information manipulation.},
  journal = {The Journal of Politics},
  number = {3}
}

